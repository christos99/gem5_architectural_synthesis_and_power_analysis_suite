XML_NAME="L2_Assoc_32_specbzip"


						## 4.4.1
OUTPUT="$XML_NAME"

NUMBER="4.4.1"


echo "Calculating Energy for $NUMBER $XML_NAME"

cd MC_PAT_RESULTS
cd $XML_FOLDER
mkdir "$XML_SUBFOLDER"
cd ../../

 MCPAT
  echo "Done with number $NUMBER"


