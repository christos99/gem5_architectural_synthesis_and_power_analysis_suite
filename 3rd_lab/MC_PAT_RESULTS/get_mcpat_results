#!/bin/sh


MCPAT()
{
./mcpat  -infile ProcessorDescriptionFiles/ALL_GEM5_CONVERTED/$XML_FOLDER/$XML_SUBFOLDER/$XML_NAME.xml -print_level 5 > MC_PAT_RESULTS/$XML_FOLDER/$XML_SUBFOLDER/$XML_NAME.txt
}


 ##print_energy()
#{
#python2 print_energy.py MC_PAT_RESULTS/$XML_FOLDER/$XML_SUBFOLDER/$XML_NAME.txt Benchmarks/Latest_Final_Benchmarks/$XML_FOLDER/$XML_SUBFOLDER/$BENCHMARK/stats.txt > EnergyResults/$XML_FOLDER/$XML_SUBFOLDER/$XML_NAME-energy.txt
#}

