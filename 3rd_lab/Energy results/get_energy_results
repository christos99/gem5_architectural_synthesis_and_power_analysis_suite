XML_NAME="L2_Assoc_8_specjeng"

BENCHMARK="specjeng"
						## 4.2.5
OUTPUT="$XML_NAME"

NUMBER="4.2.5"



echo "Calculating Energy for $NUMBER $XML_NAME"

 get_energy
  echo "Done with number $NUMBER"


