#!/bin/sh





 get_energy()
 {


python2 print_energy.py MC_PAT_RESULTS/$XML_FOLDER/$XML_SUBFOLDER/$XML_NAME.txt Benchmarks/Latest_Final_Benchmarks/$XML_FOLDER/$XML_SUBFOLDER/$BENCHMARK/stats.txt > EnergyResults/$XML_FOLDER/$XML_SUBFOLDER/$XML_NAME-energy.txt


}

################################################
################################################
################################################
################################################


XML_FOLDER="L1_Cache"

XML_SUBFOLDER="64kB"
						## 1.1.1
XML_NAME="L1_64_specbzip"

BENCHMARK="specbzip"

OUTPUT="$XML_NAME"

NUMBER="1.1.1"

echo "Calculating Energy for $NUMBER $XML_NAME"
cd EnergyResults
mkdir "$XML_FOLDER"
cd $XML_FOLDER
mkdir "$XML_SUBFOLDER"
cd ../../

 get_energy 
 echo "Done with number $NUMBER"


################################################
XML_FOLDER="L1_Cache"

